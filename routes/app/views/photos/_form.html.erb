

<%= form_with model: @photo do |form| %>
  <div>
    <%= form.label :Title %><br>
    <%= form.text_field :title %>
  </div>


  <div>
    <%= form.label :Description %><br>
    <%= form.text_area :description %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

<%= link_to "Back", photos_path%>