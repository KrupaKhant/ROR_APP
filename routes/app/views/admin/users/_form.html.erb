<h1>Put your details</h1>
<%= form_with(model: [:admin, user]) do |form| %>
  <div>
    <%= form.label :Name %>
    <%= form.text_field :name %>
  </div>
  
  <div>
    <%= form.label :Email %>
    <%= form.text_field :email %>
  </div>
  
  <%= form.submit %>
<% end %>

<%= link_to "Back", admin_users_path %>